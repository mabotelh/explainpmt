  <% collection_content(@stories) do %>
    <%= form_tag move_project_stories_path(@project) %>
      <%= render :partial => 'stories/move_stories_form', :locals => {:default => "Backlog", :selected_id => 'select1'} %>
      <%= render :partial => 'stories/stories' , :locals => {:hide_cols => [:project]}%>
      <% if @stories.size > 10 %>
        <br />
        <%= render :partial => 'stories/story_links' %>
        <%= render :partial => 'stories/move_stories_form', :locals => {:default => "Backlog", :selected_id => 'select2'} %>
      <% end %>
    </form>
  <% end %>
